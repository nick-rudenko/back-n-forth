{
	"name": "back-n-forth",
	"displayName": "Back & Forth",
	"author": {
		"name": "Nick Rudenko",
		"email": "rdnkta@gmail.com"
	},
	"repository": {
		"url": "https://github.com/nick-rudenko/back-n-forth"
	},
	"keywords": [
		"navigaton",
		"buttons",
		"browser"
	],
	"icon": "img/icons/icon.png",
	"publisher": "nick-rudenko",
	"description": "Adds go back/forward buttons for easier navigation",
	"version": "0.1.2",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.showBackBtn",
		"onCommand:extension.showForwardBtn"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.showBackBtn",
				"title": "Go Back",
				"icon": {
					"dark": "img/svg/arrow-left-dark.svg",
					"light": "img/svg/arrow-left-light.svg"
				}
			},
			{
				"command": "extension.showForwardBtn",
				"title": "Go Forward",
				"icon": {
					"dark": "img/svg/arrow-right-dark.svg",
					"light": "img/svg/arrow-right-light.svg"
				}
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "extension.showBackBtn",
					"group": "navigation@999998"
				},
				{
					"command": "extension.showForwardBtn",
					"group": "navigation@999999"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
